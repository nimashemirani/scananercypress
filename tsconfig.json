{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist",
      "rootDir": "./"
    },
    "include": ["cypress/**/*.ts", "cypress/**/*.tsx", "src/**/*.ts", "src/**/*.tsx"],
    "exclude": ["node_modules"]
  }
  